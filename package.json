    "next": "^15.0.3",
    "next-themes": "^0.4.4",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.0",
    "react": "^18.3.1",
    "react-day-picker": "8.10.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.54.1",
    "react-i18next": "^15.6.0",
    "react-pageflip": "latest",
    "react-resizable-panels": "^2.1.7",
