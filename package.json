{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "dev:clean": "npm run clean && npm run dev",
    "dev:fresh": "npm run clean && npm install && npm run dev",
    "dev:type-check": "npm run type-check && npm run dev",
    "dev:https": "next dev --experimental-https",
    "dev:host": "next dev -H 0.0.0.0",
    "dev:port": "next dev -p 3001",
    "build": "next build",
    "build:debug": "NEXT_DEBUG=1 next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint --strict",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --strict",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rimraf .next out coverage .swc",
    "clean:all": "rimraf .next out coverage .swc node_modules package-lock.json",
    "clean:logs": "find . -name '*.tsx' -o -name '*.ts' -not -path './node_modules/*' | xargs grep -l 'console\\.log' | wc -l",
    "perf:audit": "npm run build && npx @next/bundle-analyzer .next/static/chunks/*.js",
    "perf:lighthouse": "npx lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "pre-commit": "npm run type-check && npm run lint && npm run format:check && npm run test:ci",
    "postinstall": "[ -d .git ] && husky install || echo 'Skipping husky install in non-git environment'",
    "setup": "npm install && npm run db:setup && npm run db:seed",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:drop": "drizzle-kit drop",
    "db:check": "drizzle-kit check",
    "db:seed": "tsx lib/db/seed.ts",
    "db:reset": "npm run db:drop && npm run db:push && npm run db:seed",
    "db:setup": "npm run db:push && npm run db:seed",
    "db:migrate:run": "tsx lib/db/migrate.ts run",
    "db:migrate:status": "tsx lib/db/migrate.ts status",
    "env:check": "node scripts/check-env-vars.js",
    "env:example": "cp .env.example .env.local",
    "deps:check": "npm audit",
    "deps:update": "npm update",
    "deps:outdated": "npm outdated",
    "server:dev": "concurrently \"npm run dev\" \"npm run db:studio\"",
    "server:prod": "npm run build && npm run start"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.25.4",
    "@emailjs/browser": "^4.4.1",
    "@hookform/resolvers": "^3.9.1",
    "@libsql/client": "^0.15.7",
    "@radix-ui/react-accordion": "1.2.2",
