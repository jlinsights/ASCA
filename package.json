    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "embla-carousel-react": "8.5.1",
    "html2canvas": "^1.4.1",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "input-otp": "1.4.1",
    "jspdf": "^2.5.2",
